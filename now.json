{
  "name": "lorrigan-website",
  "routes": [{ "src": "/", "dest": "public/index.html" }],
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@now/node"
    },
    {
      "src": "public/**/*",
      "use": "@now/static"
    },
    {
      "src": "package.json",
      "use": "@now/static-build"
    }
  ]
}
