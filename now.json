{
  "name": "lorrigan-website",
  "routes": [
    { "src": "/", "dest": "api/page?id=index" },
    { "src": "/(?<id>[^/]+)", "dest": "api/page?id=$id" }
  ],
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@now/node"
    },
    {
      "src": "public/**/*",
      "use": "@now/static"
    },
    {
      "src": "package.json",
      "use": "@now/static-build"
    }
  ]
}
